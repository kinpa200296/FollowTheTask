@model IEnumerable<FollowTheTask.BLL.Services.Team.ViewModels.TeamMemberViewModel>


@if (Model.Count() != 0)
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
            </tr>
        }

    </table>
}
else
{
    <h4>No members found!</h4>
}